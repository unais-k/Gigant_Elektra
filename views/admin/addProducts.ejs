    <body>
        <!-- PAGE LOADING
    ================================================== -->
        <div id="preloader"></div>

        <!-- PAGE CONTAINER
    ================================================== -->
        <div class="page-container">
            <!-- PAGE SIDEBAR
        ================================================== -->
            <%- include("../includes/admin-sidebar.ejs") %>
            <!-- PAGE CONTENT
        ================================================== -->
            <div class="page-content">
                <!-- PAGE HEADER
            ================================================== -->
                <%- include("../includes/admin_header.ejs") %>

                <!-- Take this much code to every page -->
                <!-- PAGE INNER
            ================================================== -->
                <div class="page-inner">
                    <!-- PAGE MAIN WRAPPER
                ================================================== -->
                    <div id="main-wrapper">
                        <!-- row -->
                        <div class="row g-xl-3">
                            <!-- <div class="col-sm-2"></div> -->
                            <div class="col-xl-12 grid-margin">
                                <div class="card card-white mb-3">
                                    <div class="card-heading clearfix">
                                        <h4 class="card-title">Product information</h4>
                                    </div>
                                    <div class="card-body">
                                        <form method="post" action="/admin/addProductPost" enctype="multipart/form-data">
                                            <div class="row">
                                                <div class="col-md-12 mb-3">
                                                    <label for="validationCustom01">Name</label>
                                                    <input
                                                        type="text"
                                                        class="form-control"
                                                        name="productname"
                                                        id="validationCustom01"
                                                        required
                                                    />
                                                    <div class="valid-feedback">Looks good!</div>
                                                </div>

                                                <div class="col-md-6 mb-3">
                                                    <label for="validationCustom02">Price</label>
                                                    <input
                                                        type="text"
                                                        class="form-control"
                                                        name="price"
                                                        id="validationCustom02"
                                                        required
                                                    />
                                                    <div class="valid-feedback">Looks good!</div>
                                                </div>
                                                <div class="col-md-6 mb-3">
                                                    <label for="validationCustom03">Details</label>
                                                    <input
                                                        type="text"
                                                        name="details"
                                                        class="form-control"
                                                        id="validationCustom03"
                                                        required
                                                    />
                                                    <div class="valid-feedback">Looks good!</div>
                                                </div>
                                                <div class="col-md-6 mb-4">
                                                    <label for="validationCustom09">Category</label>
                                                    <select
                                                        class="form-select form-control"
                                                        id="selectID"
                                                        name="category"
                                                        onchange="selectVale()"
                                                        required
                                                        aria-label="Default select example"
                                                    >
                                                        <option disabled value="choose one">Choose one</option>
                                                        <%for( let i=0; i < categoryList.length; i ++ ) { %>
                                                        <option value="<%=categoryList[i].categoryname%>">
                                                            <%= categoryList[i].categoryname %>
                                                        </option>
                                                        <% } %>
                                                    </select>
                                                </div>

                                                <div class="col-md-6 mb-3">
                                                    <label for="validationCustom05">Quantity</label>
                                                    <input
                                                        type="text"
                                                        name="quantity"
                                                        class="form-control"
                                                        id="validationCustom05"
                                                        required
                                                    />
                                                    <div class="valid-feedback">Looks good!</div>
                                                </div>
                                                <div class="col-md-6 mb-3">
                                                    <label for="validationCustom01">Brand</label>
                                                    <select
                                                        class="form-select form-control"
                                                        id="brandop"
                                                        required
                                                        name="brand"
                                                        aria-label="Default select example"
                                                    >
                                                        <option selected value="choose one">Choose one Category</option>
                                                    </select>
                                                    <div class="valid-feedback">Looks good!</div>
                                                </div>

                                                <div class="col-md-6 mb-4">
                                                    <label for="validationCustom09">Colour</label>
                                                    <select
                                                        class="form-select form-control"
                                                        id="validationCustom09"
                                                        name="color"
                                                        aria-label="Default select example"
                                                    >
                                                        <option>red</option>
                                                        <option>white</option>
                                                        <option>blue</option>
                                                        <option>black</option>
                                                        <option>purple</option>
                                                    </select>
                                                </div>
                                                <!-- <div class="card-heading clearfix mt-3">
                                                    <h4 class="card-title">Thumbnail image</h4>
                                                </div>
                                                <div class="col-md-12 mb-5">
                                                    <div>
                                                        <input
                                                            class="form-control form-control-lg"
                                                            id="formFileLg"
                                                            type="file"
                                                            accept="image/jpeg,image/gif,image/png,image/x-eps"
                                                            name="thumbnail"
                                                        />
                                                    </div>
                                                </div> -->

                                                <div class="card-heading clearfix">
                                                    <h4 class="card-title">Photos</h4>
                                                </div>
                                                <div class="col-md-12">
                                                    <div>
                                                        <input
                                                            class="form-control form-control-lg"
                                                            id="formFileLg"
                                                            type="file"
                                                            required
                                                            accept="image/jpeg,image/gif,image/png,image/x-eps"
                                                            name="imageName"
                                                        />
                                                    </div>
                                                    <br />
                                                    <div>
                                                        <input
                                                            class="form-control form-control-lg"
                                                            id="formFileLg"
                                                            type="file"
                                                            required
                                                            accept="image/jpeg,image/gif,image/png,image/x-eps"
                                                            name="imageName"
                                                        />
                                                    </div>
                                                    <br />
                                                    <div>
                                                        <input
                                                            class="form-control form-control-lg"
                                                            id="formFileLg"
                                                            type="file"
                                                            required
                                                            accept="image/jpeg,image/gif,image/png,image/x-eps"
                                                            name="imageName"
                                                        />
                                                    </div>
                                                </div>
                                                <div class="col-md-12">
                                                    <div class="text-center m-3">
                                                        <button type="submit" class="btn btn-primary">SUBMIT</button>
                                                    </div>
                                                </div>
                                                <% if (message) { %>
                                                <h5 style="color: red"><%= message %></h5>
                                                <% } else { %> <% } %>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- end row -->
