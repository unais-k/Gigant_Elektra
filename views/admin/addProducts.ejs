        <!-- axios cdn -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/1.2.3/axios.min.js"></script>

        <!-- sweet alert cdn -->
        <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>

        <script>
            function selectVale() {
                let value = document.getElementById("selectID").value;
                axios({
                    method: "post",
                    url: "/admin/catefind",
                    data: {
                        value: value,
                    },
                })
                    .then((res) => {
                        if (res) {
                            let html = "";
                            let brand = res.data;
                            console.log(brand.Brand);
                            for (let i = 0; i < brand.Brand.length; i++) {
                                html += `<option>${brand.Brand[i]}</option>`;
                            }
                            document.getElementById("brandop").innerHTML = html;
                        }
                    })
                    .catch((err) => {
                        console.log(err);
                    });
            }
        </script>

