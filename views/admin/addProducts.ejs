            <div class="page-content">
                <!-- PAGE HEADER
            ================================================== -->
                <div class="page-header">
                    <div class="search-form">
                        <form action="/admin/addProducts" method="post">
                            <div class="input-group">
                                <input
                                    type="text"
                                    name="search"
                                    class="form-control search-input"
                                    placeholder="Type something..."
                                />
                                <span class="input-group-btn">
                                    <button class="btn btn-default" id="close-search" type="button">
                                        <i class="icon-close"></i>
                                    </button>
                                </span>
                            </div>
                        </form>
                    </div>
                    <nav class="navbar navbar-default">
                        <div class="container-fluid">
                            <!-- Brand and toggle get grouped for better mobile display -->
                            <div class="navbar-header">
                                <div class="logo-sm">
                                    <a href="#" id="sidebar-toggle-button"><i class="fa fa-bars"></i></a>
                                    <a class="logo-box" href="index.html"
                                        ><img src="/images/admin/logo-no-background.png" class="logo" alt="..."
                                    /></a>
                                </div>
                                <button
                                    type="button"
                                    class="navbar-toggle collapsed"
                                    data-bs-toggle="collapse"
                                    data-bs-target="#bs-example-navbar-collapse-1"
                                    aria-expanded="false"
                                >
